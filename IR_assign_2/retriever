   "source": [
    "import json\n",
    "import os\n",
    "import pandas as pd\n",
    "def read_fever_jsonl(file_path):\n",
    "    examples = []\n",
    "    with open(file_path, 'r', encoding='utf-8') as f:\n",
    "        for line in f:\n",
    "            # try:\n",
    "            data = json.loads(line)\n",
    "            example = {\n",
    "                \"id\": data[\"id\"],\n",
    "                \"label\": data[\"label\"],\n",
    "                \"claim\": data[\"claim\"],\n",
    "                \"evidence\": []\n",
    "            }\n",
    "            for evidence_group in data[\"evidence\"]:\n",
    "                evidence_group_list = []\n",
    "                for evidence in evidence_group:\n",
    "                    annotation_id, evidence_id, entity, position = evidence\n",
    "                    evidence_group_list.append([annotation_id, evidence_id, entity, position])\n",
    "                example[\"evidence\"].append(evidence_group_list)\n",
    "            examples.append(example)\n",
    "    return examples\n",
    "\n",
    "file_path = \"./train.jsonl\"\n",
    "examples = read_fever_jsonl(file_path)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Directory containing the JSON files\n",
    "directory = \"./wiki-pages/\"\n",
    "\n",
    "# List to store data from all files\n",
    "data_list = {}\n",
    "sentence_list = {}\n",
    "\n",
    "# Iterate over each file in the directory\n",
    "for filename in os.listdir(directory):\n",
    "    if filename.endswith(\".jsonl\"):\n",
    "        file_path = os.path.join(directory, filename)\n",
    "        with open(file_path, 'r') as file:\n",
    "            for line in file:\n",
    "                json_data = json.loads(line)\n",
    "                id = json_data.get(\"id\", \"\")\n",
    "                text = json_data.get(\"text\", \"\")\n",
    "                lines = json_data.get(\"lines\", \"\")\n",
    "                data_list[text]= {\"sentence_id\": id, \"text\": text, \"lines\": {i: line[2:] for i, line in enumerate(lines.replace('\\t', ' ').split('\\n'))}}\n",
    "                for i, line in enumerate(lines.replace('\\t', ' ').split('\\n')):\n",
    "                    if(line[2:] != ''):\n",
    "                        sentence_list[line[2:]] = [id, i]"
   ]
