    "def read_fever_test_jsonl(file_path):\n",
    "    examples = []\n",
    "    with open(file_path, 'r', encoding='utf-8') as f:\n",
    "        for line in f:\n",
    "            # try:\n",
    "            data = json.loads(line)\n",
    "            example = {\n",
    "                \"id\": data[\"id\"],\n",
    "                \"claim\": data[\"claim\"]\n",
    "            }\n",
    "            examples.append(example)\n",
    "    return examples\n",
    "\n",
    "file_path = \"./shared_task_dev.jsonl\"\n",
    "examples = read_fever_test_jsonl(file_path)"
